<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900' rel='stylesheet' type='text/css'>

    <!-- Page title -->
    <title>LikePanel | Накрутка лайков</title>

    <!-- Vendor styles -->
    <link rel="stylesheet" href="vendor\fontawesome\css\font-awesome.css">
    <link rel="stylesheet" href="vendor\animate.css\animate.css">
    <link rel="stylesheet" href="vendor\bootstrap\css\bootstrap.css">
    <link rel="stylesheet" href="vendor\toastr\toastr.min.css">

    <!-- App styles -->
    <link rel="stylesheet" href="styles\pe-icons\pe-icon-7-stroke.css">
    <link rel="stylesheet" href="styles\pe-icons\helper.css">
    <link rel="stylesheet" href="styles\stroke-icons\style.css">
    <link rel="stylesheet" href="styles\style.css">
</head>
<body>

<!-- Wrapper-->
<div class="wrapper">

    <!-- Header-->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <div id="mobile-menu">
                    <div class="left-nav-toggle">
                        <a href="#">
                            <i class="stroke-hamburgermenu"></i>
                        </a>
                    </div>
                </div>
                <a class="navbar-brand" href="index.htm">
                    Like
                    <span>v.1.0</span>
                </a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <div class="left-nav-toggle">
                    <a href="">
                        <i class="stroke-hamburgermenu"></i>
                    </a>
                </div>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="versions.htm">Баланс
                            <span class="label-balance label-warning">
                                <%= (user.balance / 100).toFixed(2) %>₽
                            </span>
                        </a>
                    </li>
                    <li class=" profil-link">
                        <a href="login.htm">
                            <span class="profile-address"><%= user.username %></span>
                            <img src="images\profile.jpg" class="img-circle" alt="">
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- End header-->

    <!-- Navigation-->
    <aside class="navigation">
        <nav>
            <ul class="nav luna-nav">
                <li class="nav-category">
                    Основное меню
                </li>
                <li>
                    <a href="index">Главная</a>
                </li>
                <li>
                    <a href="#uielements" data-toggle="collapse" aria-expanded="false">
                        Накрутка<span class="sub-nav-icon"> <i class="stroke-arrow"></i> </span>
                    </a>
                    <ul id="uielements" class="nav nav-second collapse">
                        <li><a href="tasks">Все задачи</a></li>
                        <li><a href="addtask">Создать задачу</a></li>
                    </ul>
                </li>
                <li>
                    <a href="profile">Профиль</a>
                </li> 
                <li>
                    <a href="news">Новости</a>
                </li>
                <li>
                    <a href="support">Помощь</a>
                </li>
                <% if (user.admin) { %>
                    <li class="active">
                        <a href="admin">Админка</a>
                    </li>
                <% } %>

                <li class="nav-info">
                    <div class="m-t-xs">
                        <span class="c-white">LikePanel</span> сервис для накрутки лайков.
                    </div>
                </li>
            </ul>
        </nav>
    </aside>
    <!-- End navigation-->


    <!-- Main content-->
    <section class="content">
        <div class="container-fluid">

            <div class="row">
                <div class="col-lg-12">
                    <div class="view-header">
                        <div class="header-icon">
                            <i class="pe page-header-icon pe-7s-shield"></i>
                        </div>
                        <div class="header-title">
                            <h3 class="m-b-xs">Админка</h3>
                            <small>
                                Основная статистика и функции сайта
                            </small>
                        </div>
                    </div>
                    <hr>
                </div>
            </div>

            <div class="row">

                <div class="col-lg-3 col-xs-6">
                    <div class="panel panel-filled">
                        <div class="panel-body">
                            <h2 class="m-b-none">
                                206
                            </h2>
                            <div class="small">Всего пользователей</div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-xs-6">
                    <div class="panel panel-filled">
                        <div class="panel-body">
                            <h2 class="m-b-none">
                                140.00₽
                            </h2>
                            <div class="small">Всего пополнено</div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-xs-6">
                    <div class="panel panel-filled">

                        <div class="panel-body">
                            <h2 class="m-b-none">
                                206
                            </h2>
                            <div class="small">Задач создано</div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-xs-6">
                    <div class="panel panel-filled">

                        <div class="panel-body">
                            <h2 class="m-b-none">
                                todo
                            </h2>
                            <div class="small">todo</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="panel panel-filled">
                        <div class="panel-body">
                            <h4> Задач создано</h4>
                            <div class="text-center slight">
                            </div>

                            <div class="flot-chart" style="height: 160px;margin-top: 5px">
                                <div class="flot-chart-content" id="tasks-create"></div>
                            </div>

                            <div class="small text-center">Все созданные задачи за последний месяц.</div>
                        </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="panel panel-filled">
                            <div class="panel-body">
                                <h4> Пополнение баланса</h4>
                                <div class="text-center slight">
                                </div>

                                <div class="flot-chart" style="height: 160px;margin-top: 5px">
                                    <div class="flot-chart-content" id="refill-balance"></div>
                                </div>

                                <div class="small text-center">Пополнение баланса за последний месяц.</div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="panel panel-filled">
                            <div class="panel-body">
                                <h4> Добавить аккаунты вк</h4>
                                <div class="form-group">
                                    <textarea id="accounts" class="form-control" rows="3" placeholder="Строки в формате login:password"></textarea>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-6 col-md-offset-4">
                                        <a id="add-account" class="btn btn-w-md btn-accent" style="align: center">Добавить</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="panel panel-filled">
                            <div class="panel-body">
                                <h4> Пополнить баланс</h4>

                                <div class="form-group">
                                    <p>Логин пользователя</p>
                                    <input type="text" class="form-control" id="username" placeholder="Логин">
                                </div>

                                <div class="form-group">
                                    <p>Тип операции</p>
                                    <select id="operation-type" class="form-control">
                                        <option value="add">Добавить
                                        <option value="sub">Отнять
                                    </select>
                                </div>
                                <div class="form-group">
                                    <p>Количество (копеек)</p>
                                    <input type="number" class="form-control" id="money-count" placeholder="1">
                                </div>
                                <div class="form-group">
                                    <div class="col-md-6 col-md-offset-4">
                                    <a id="change-balance" class="btn btn-w-md btn-accent" style="align: center">Пополнить</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End main content-->

</div>
<!-- End wrapper-->

<!-- Vendor scripts -->
<script src="vendor\pacejs\pace.min.js"></script>
<script src="vendor\jquery\dist\jquery.min.js"></script>
<script src="vendor\bootstrap\js\bootstrap.min.js"></script>
<script src="vendor\toastr\toastr.min.js"></script>
<script src="vendor\sparkline\index.js"></script>
<script src="vendor\flot\jquery.flot.min.js"></script>
<script src="vendor\flot\jquery.flot.resize.min.js"></script>
<script src="vendor\flot\jquery.flot.spline.js"></script>
<script src="vendor\flot\jquery.flot.time.js"></script>

<!-- App scripts -->
<script src="scripts\luna.js"></script>
<script src="scripts\admin.js"></script>

</body>

</html>